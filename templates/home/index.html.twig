    <section id="contact">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Contactez-moi</h2>
                    <h3 class="section-subheading text-muted">
                        {% set successMessages = app.flashes('success') %}
                        {% set errorMessages = app.flashes('error') %}

                        {% if successMessages|length > 0 %}
                            <div class="alert alert-success">
                                {{ successMessages[0] }}
                            </div>
                        {% endif %}

                        {% if errorMessages|length > 0 %}
                            <div class="alert alert-danger">
                                {{ errorMessages[0] }}
                            </div>
                        {% endif %}
                    </h3>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    {{ form_start(form) }}
                    <form name="sentMessage" id="contactForm" method="post" action="#contact">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    {{ form_widget(form.firstname, {'label': false, 'attr': {'class': 'form-control', 'placeholder': 'Votre prénom'}}) }}
                                    {% if form_errors(form.firstname) %}
                                        <div class="erreur">
                                            {{ form_errors(form.firstname) }}
                                        </div>
                                    {% endif %}
                                </div>
                                <div class="form-group">
                                    {{ form_widget(form.name, {'label': false, 'attr': {'class': 'form-control', 'placeholder': 'Votre nom'}}) }}
                                    {% if form_errors(form.name) %}
                                        <div class="erreur">
                                            {{ form_errors(form.name) }}
                                        </div>
                                    {% endif %}
                                </div>
                                <div class="form-group">
                                    {{ form_widget(form.email, {'label': false, 'attr': {'class': 'form-control', 'placeholder': 'Votre e-mail'}}) }}
                                    {% if form_errors(form.email) %}
                                        <div class="erreur">
                                            {{ form_errors(form.email) }}
                                        </div>
                                    {% endif %}
                                </div>
                                <div class="form-group">
                                    {{ form_widget(form.phone, {'label': false, 'attr': {'class': 'form-control', 'placeholder': 'Votre numéro de téléphone'}}) }}
                                    {% if form_errors(form.phone) %}
                                        <div class="erreur">
                                            {{ form_errors(form.phone) }}
                                        </div>
                                    {% endif %}
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    {{ form_widget(form.message, {'label': false, 'attr': {'class': 'form-control', 'placeholder': 'Votre message'}}) }}
                                    {% if form_errors(form.message) %}
                                        <div class="erreur">
                                            {{ form_errors(form.message) }}
                                        </div>
                                    {% endif %}
                                </div>
                            </div>
                            {{ form_row(form._token) }}
                            <div class="clearfix"></div>
                            <div class="col-lg-12 text-center">
                                <div class="g-recaptcha" data-sitekey="6LeCRUkoAAAAAH1eA_pLhiSKbdXAyyiVQ9sdXI3W"></div>
                                <div id="success"></div>
                                <button type="submit" class="btn btn-xl">{{ button_label|default('Envoyer') }}</button>
                            </div>
                        </div>
                    </form>
                    {{ form_end(form) }}
                </div>
            </div>
        </div>
    </section>
{% endblock %}
