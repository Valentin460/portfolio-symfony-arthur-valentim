{% extends 'base.html.twig' %}
{% block body %}
    <header>
        <div class="container">
            <div class="intro-text">
                <img src="{{ asset('uploads/images/Arthur.jpg') }}" class="profile-photo">
                <div class="intro-lead-in">Arthur VALENTIM</div>
                <div class="intro-heading">Développeur full-stack junior, actuellement en mastère ingénierie du web au sein de l'ESGI à Paris.</div>
                <a href="{{ asset('uploads/files/CV_Arthur_Valentim.pdf') }}" download="CV_Arthur_Valentim.pdf" class="page-scroll btn btn-cv-xl">Télécharger mon CV (207Ko) <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-download" viewBox="0 0 16 16">
                        <path d="M.5 9.9a.5.5 0 0 1 .5.5v2.5a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1v-2.5a.5.5 0 0 1 1 0v2.5a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2v-2.5a.5.5 0 0 1 .5-.5z"/>
                        <path d="M7.646 11.854a.5.5 0 0 0 .708 0l3-3a.5.5 0 0 0-.708-.708L8.5 10.293V1.5a.5.5 0 0 0-1 0v8.793L5.354 8.146a.5.5 0 1 0-.708.708l3 3z"/>
                    </svg></a>
            </div>
        </div>
    </header>

    <section id="portfolio" class="bg-light-gray">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Réalisations</h2>
                    <h3 class="section-subheading text-muted"></h3>
                </div>
            </div>
            <div class="row">
                {% for project in projects %}
                <div class="col-md-4 col-sm-6 portfolio-item">
                    <a href="{{ path('app_project_show', {'id': project.id}) }}" class="portfolio-link" data-toggle="modal">
                        <img src="{{ asset('uploads/images/' ~ project.picture) }}" class="img-responsive" alt="">
                    </a>
                    <div class="portfolio-caption">
                        <h4>{{ project.title }}</h4>
                        <p class="text-muted">{{ project.thematic }}</p>
                    </div>
                </div>
                {% endfor %}
            </div>
            <div class="row">
                {% for project in projects %}
                <div class="col-sm-6 col-md-4">
                    <div class="thumbnail">
                        <a href="{{ path('app_project_show', {'id': project.id}) }}"><img src="{{ asset('uploads/images/' ~ project.picture) }}" alt="..."></a>
                        <div class="caption">
                            <h3>{{ project.title }}</h3>
                            <p>{{ project.thematic }}</p>
                        </div>
                    </div>
                </div>
                {% endfor %}
            </div>
        </div>
    </section>

    <section id="about">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
